{
  "name": "react-webpack-typescript-starter",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/* && cross-env NODE_ENV=production webpack -p --config=configs/webpack/production.js",
    "lint": "eslint './src/**/*.ts*' './tests/**/*.test.ts*' --cache",
    "prettier": "prettier  --ignore-path ./configs/.prettierignore  **/*.ts*",
    "fix": "yarn prettier --write && yarn lint --fix",
    "tsc": "tsc",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=configs/webpack/development.js",
    "start": "node server.js",
    "test": "jest --coverage --config=configs/jest.json",
    "check-rule": "yarn lint && yarn prettier && yarn tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "1.x",
    "@typescript-eslint/parser": "1.x",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "10.x",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "6.x",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "file-loader": "^4.2.0",
    "fork-svg-loader": "^0.0.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.12",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@kenshooui/react-multi-select": "^1.1.5",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "deep-equal": "^1.1.0",
    "express": "^4.17.1",
    "lodash.debounce": "^4.0.8",
    "lodash.pick": "^4.4.0",
    "rc-tooltip": "^3.7.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-images": "^1.0.0",
    "react-router-dom": "^5.0.1",
    "react-skylight": "^0.5.1",
    "styled-components": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn check-rule"
    }
  }
}
