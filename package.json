    "react-autocomplete": "^1.8.1",
    "react-collapsible": "^2.6.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn check-rule"
    }
  }
